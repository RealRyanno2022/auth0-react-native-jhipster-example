{
  "name": "flickr-2",
  "version": "0.0.1",
  "scripts": {
    "android": "expo start --android",
    "build:android": "expo build:android",
    "build:ios": "expo build:ios",
    "build:web": "expo export:web",
    "coverage": "jest --coverage && open coverage/lcov-report/index.html || xdg-open coverage/lcov-report/index.html",
    "postinstall": "patch-package",
    "ios": "expo start --ios",
    "lint": "eslint . --ext .js,.ts,.jsx,.tsx",
    "precommit": "lint-staged",
    "prettier": "prettier --write \"{,.,**/,.jhipster/**/}*.{md,json,yml,js,ts,tsx}\"",
    "publish": "expo publish",
    "start": "expo start",
    "start:e2e": "expo start --no-dev --minify",
    "test": "jest",
    "test:e2e": "./e2e/scripts/setup.sh && detox test --configuration ios.sim.release --retries 3",
    "test:watch": "jest --watch",
    "updateSnapshot": "jest --updateSnapshot",
    "web": "expo start --web"
  },
  "lint-staged": {
    "{,app/**/,test/**/}*.{js,json,yml,md,ts,tsx}": [
      "prettier --write",
      "eslint --fix --ext .js"
    ]
  },
  "jest": {
    "coveragePathIgnorePatterns": [
      "app/shared/services/api.js",
      "app/shared/themes/",
      "/storybook/",
      "/*\\.story\\.js"
    ],
    "moduleNameMapper": {
      "^.+\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "identity-obj-proxy"
    },
    "preset": "jest-expo",
    "setupFiles": [
      "./test/setup"
    ],
    "testMatch": [
      "**/test/**/*.js",
      "**/?(*.)(spec|test).js?(x)"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/e2e/",
      "test/setup.js"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(@react-native-community|@react-native|react-native|expo(nent)?|@expo(nent)?/.*|react-navigation|@react-navigation/.*|@unimodules/.*|@storybook))"
    ]
  },
  "dependencies": {
    "@expo/vector-icons": "^13.0.0",
    "@react-native-async-storage/async-storage": "~1.17.10",
    "@react-native-community/datetimepicker": "6.2.0",
    "@react-native-masked-view/masked-view": "0.2.7",
    "@react-native-picker/picker": "2.4.2",
    "@react-navigation/devtools": "6.0.4",
    "@react-navigation/drawer": "5.12.5",
    "@react-navigation/native": "5.9.4",
    "@react-navigation/stack": "5.14.5",
    "apisauce": "2.1.2",
    "expo": "~46.0.9",
    "expo-auth-session": "~3.7.1",
    "expo-constants": "~13.2.4",
    "expo-image-picker": "13.3.1",
    "expo-linking": "~3.2.2",
    "expo-random": "~12.3.0",
    "expo-splash-screen": "~0.16.2",
    "expo-status-bar": "~1.4.0",
    "expo-web-browser": "~11.0.0",
    "format-json": "1.0.3",
    "formik": "2.2.9",
    "identity-obj-proxy": "3.0.0",
    "lodash": "4.17.21",
    "querystringify": "2.2.0",
    "ramda": "0.27.1",
    "react": "18.0.0",
    "react-datepicker": "4.3.0",
    "react-dom": "18.0.0",
    "react-native": "0.69.5",
    "react-native-gesture-handler": "~2.5.0",
    "react-native-keyboard-aware-scroll-view": "0.9.5",
    "react-native-modal-datetime-picker": "13.1.2",
    "react-native-picker-select": "8.0.4",
    "react-native-reanimated": "~2.9.1",
    "react-native-safe-area-context": "4.3.1",
    "react-native-screens": "~3.15.0",
    "react-native-sectioned-multi-select": "0.8.1",
    "react-native-web": "^0.18.9",
    "react-redux": "7.2.6",
    "redux": "4.1.2",
    "redux-persist": "6.0.0",
    "redux-saga": "1.1.3",
    "reduxsauce": "1.2.1",
    "seamless-immutable": "7.1.4",
    "yup": "0.32.11"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@expo/webpack-config": "^0.17.2",
    "@react-native-community/eslint-config": "3.0.1",
    "@ruddell/detox-expo-helpers": "0.7.0",
    "@storybook/addons": "5.3.21",
    "@storybook/react-native": "5.3.25",
    "@storybook/theming": "5.3.21",
    "@types/react": "~18.0.14",
    "@types/react-native": "~0.69.1",
    "babel-jest": "27.4.2",
    "babel-preset-env": "1.7.0",
    "buffer": "6.0.3",
    "detox": "19.11.0",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.6",
    "eslint": "7.30.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-prettier": "4.0.0",
    "expect": "27.4.2",
    "expo-detox-hook": "1.0.10",
    "generator-jhipster-react-native": "4.2.5",
    "jest": "26.6.3",
    "jest-circus": "26.6.3",
    "jest-expo": "^46.0.1",
    "lint-staged": "12.1.2",
    "mockery": "2.1.0",
    "patch-package": "6.4.7",
    "path-exists": "5.0.0",
    "prettier": "2.5.1",
    "react-test-renderer": "18.2.0",
    "redux-devtools-extension": "2.13.9",
    "resolve": "1.20.0",
    "rimraf": "3.0.2",
    "tapable": "2.2.1",
    "typescript": "4.6.3"
  },
  "engines": {
    "node": ">=14.x",
    "npm": ">=6.x"
  }
}
